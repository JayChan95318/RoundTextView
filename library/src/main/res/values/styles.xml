<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--支持圆角的TextView-->
    <declare-styleable name="RoundTextView">
        <attr name="tvBackgroundColor" format="color"/>
        <attr name="tvBorderWidth" format="dimension"/>
        <attr name="tvBorderColor" format="color"/>
        <attr name="tvCornerRadius" format="dimension"/>
        <attr name="tvTopLeftRadius" format="dimension"/>
        <attr name="tvTopRightRadius" format="dimension"/>
        <attr name="tvBottomLeftRadius" format="dimension"/>
        <attr name="tvBottomRightRadius" format="dimension"/>
    </declare-styleable>



</resources>